<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.column {
			padding: 0px 5px;
		}
		.panel {
			margin: 5px;
		}
	</ui:style>
  <g:HorizontalPanel addStyleNames='{style.panel}'>
      <g:HTMLPanel addStyleNames='{style.column}'>
      <g:Label>First name contains:</g:Label>
      <g:TextBox ui:field='firstNameTextBox' />
      <g:Label>Last name contains:</g:Label>
      <g:TextBox ui:field='lastNameTextBox' />
      <g:Label>Email contains:</g:Label>
      <g:TextBox ui:field='emailTextBox' />
      <g:Label>Organization contains:</g:Label>
      <g:TextBox ui:field='organizationTextBox' />
    </g:HTMLPanel>
    <g:HTMLPanel>
      <g:Label>Enabled:</g:Label>
      <g:ListBox ui:field='enabledListBox' visibleItemCount='1' />
      <g:Label>Can create campaigns:</g:Label>
      <g:ListBox ui:field='canCreateListBox' visibleItemCount='1' />
      <g:Label>Is an admin:</g:Label>
      <g:ListBox ui:field='adminListBox' visibleItemCount='1' />
    </g:HTMLPanel>
  </g:HorizontalPanel>
</ui:UiBinder> 